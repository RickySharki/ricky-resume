{
  "name": "ricky-vite-tempalte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:ricky": "vue-tsc && vite build --mode ricky",
    "build:yc": "vue-tsc && vite build --mode yc",
    "build:ln": "vue-tsc && vite build --mode ln",
    "preview": "vite preview",
    "lint": "lint-staged",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/lodash-es": "^4.17.6",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "aos": "^2.3.4",
    "axios": "^1.2.2",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.28",
    "highlight.js": "^11.8.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@emotion/css": "^11.11.0",
    "@iconify/vue": "^4.1.1",
    "@types/aos": "^3.0.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^9.10.0",
    "@vueuse/integrations": "^9.10.0",
    "@vueuse/router": "^9.10.0",
    "animate.css": "^4.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.3",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "pinia-plugin-persistedstate": "^3.0.2",
    "qs": "^6.11.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.15.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vite-plugin-windicss": "^1.8.10",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^1.0.11",
    "windicss": "^3.5.6"
